#%Module1.0#####################################################################
## fimex 0.63.3 modulefile
##
proc ModulesHelp { } {
   puts stderr "\tfimex-0.63.3-metno"
}


module-whatis   "fimex 0.63.3-metno"
set              root             /modules/trusty/user-apps/fimex/0.63.3
prepend-path     PATH            $root/bin
prepend-path     LD_LIBRARY_PATH $root/lib
prepend-path     PKG_CONFIG_PATH $root/lib/pkgconfig
# the settings for CFLAGS and LDFLAGS are generated by pkg-config
prepend-path -d " " CFLAGS        -I/modules/trusty/user-apps/fimex/0.63.3/include -I/usr/include/libxml2  
prepend-path -d " " LDFLAGS       -fopenmp -L/modules/trusty/user-apps/fimex/0.63.3/lib -lfimex  
