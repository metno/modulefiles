#%Module1.0#####################################################################
##
## CDO modulefile

set modulefile [lrange [split [module-info name] {/}] 0 0]
set apphome   /prod/forecast/emep/cwfemep/modules
set appname   "cams50"
set appurl    http://publicwiki.meteo.fr/confluence/display/CAMS50

set modulefile [lrange [split [module-info name] {/}] 0 0]
set release    [lrange [split [module-info name] {/}] 1 1]

proc ModulesDisplay {} {
  global appname release apphome

  puts stderr "\nLoads $appname version $release"
}

proc ModulesHelp { } {
  global appname appurl modulefile apphome release

  puts stderr "The $modulefile modulefile defines the default system paths and"
  puts stderr "environment variables needed to use $appname release $release.\n"
  puts stderr "Documentation on $appurl.\n"
}

module-whatis   "EMEP-MSC/W setup for CAMS50 project"

##set root	$apphome/$appname.$release

#odule purge
module load intelcomp/13.0.1
module load mpt/2.06
module load netcdf/4.3.0
module load fftw/3.3.3
module load emep-ctm/3064p2.CWF
#odule load python/2.7.3 grib/1.10.4 cdo/1.6.0 ncl/6.1.2 nco/4.3.1

