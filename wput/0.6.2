proc ModulesHelp { } {
   puts stderr "\wput version 0.6.2"
}

module-whatis   "Wput is a command-line ftp-client that looks like wget but instead of downloading, uploads files or whole directories to remote ftp-servers."
set              root            /modules/xenial/user-apps/wput/0.6.2
prepend-path     PATH            $root/bin
prepend-path     MANPATH         $root/share/man
